<h1>Users</h1>

<table class="table">
    <thead>
        <tr>
           
            <th>User Name</th>
            <th>Password</th>
        </tr>
    </thead>
    <tbody>
        <% @users.each do |user| %>
            <tr>
                
                <td><%= user.username %></td>
                <td><%= user.password %></td>
               
            </tr>
        <% end %>
    </tbody>
</table>